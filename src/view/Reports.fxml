                     <VBox.margin>
                        <Insets top="-1.0" />
                     </VBox.margin>
                  </Button>
                  <Button fx:id="reportsButton" mnemonicParsing="false" prefHeight="60.0" prefWidth="228.0" style="-fx-background-color: #352243;" text="REPORTS" textFill="WHITE">
                     <font>
                        <Font name="System Bold Italic" size="16.0" />
