            <Label fx:id="headerText" layoutX="650.0" layoutY="100.0" style="-fx-background-color: #dddddd;" text="REPORTS" textFill="#545454">
               <font>
                  <Font name="System Bold Italic" size="18.0" />
               </font>
               <padding>
                  <Insets left="20.0" right="20.0" />
               </padding>
            </Label>
         </children>
      </AnchorPane>
   </children>
</GridPane>
